@model LibraryManagment.Areas.Admin.Models.ResponseModel
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@if (Model != null)
{
    <div class="alert alert-dismissible @Model.StyleCssClass">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
        <h5><i class="icon fas @Model.IconCssClass"></i> @Model.Title</h5>
        @Model.Message
    </div>
}